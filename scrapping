import requests
from bs4 import BeautifulSoup

# Replace this with the URL of the webpage you want to scrape
url = "https://www.daraz.pk/"

# Send an HTTP GET request to the URL
response = requests.get(url)

# Check if the request was successful (status code 200)
if response.status_code == 200:
    # Parse the HTML content of the webpage using BeautifulSoup
    soup = BeautifulSoup(response.text, "html.parser")

    # Find and extract all the text within <p> tags or any other HTML elements as needed
    text = ""
    for paragraph in soup.find_all('p'):
        text += paragraph.get_text() + "\n"

    # Print the clean text to the console
    print(text)
else:
    print("Failed to retrieve the webpage. Status code:", response.status_code)
import random

# Generate a random integer between a range
random_integer = random.randint(1, 100)
print("Random Integer:", random_integer)

# Generate a random float between 0 and 1
random_float = random.random()
print("Random Float:", random_float)

# Generate a random float within a specified range
random_float_range = random.uniform(1.0, 10.0)
print("Random Float in Range:", random_float_range)

# Select a random item from a list    
my_list = ["apple", "banana", "cherry", "date"]
random_item = random.choice(my_list)
print("Random Item from List:", random_item)

# Shuffle the order of elements in a list
random.shuffle(my_list)
print("Shuffled List:", my_list)


